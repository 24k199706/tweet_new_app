<div class="main posts-show">
  <div class="container">
    <div class="posts-show-item">
      <p>
        <%= @post.content %>
      <p>
      <div class="post-time">
        <!-- @postのcreated_atカラムのデータを表示してください -->
        <%= @post.created_at %>
      </div>
      <div class="post-menus">
        <%= link_to("編集", edit_path) %>
        <%= link_to("削除", destroy_path , method:"post") %>
      </div>
    </div>
  </div>
</div>